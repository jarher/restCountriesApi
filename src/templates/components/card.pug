each country in data 
    -var countryName= country.name.official 
    -var flag= country.flags.svg || country.flags.png

    a.card(href="#/country/" + countryName)
        img.card-image(src=flag alt="flag of " + countryName)
        div.card-content
            span.card-title= countryName
            p
                span.item-name Population:
                span.item-value= country.population
            p
                span.item-name Region:
                span.item-value= country.region
            p
                span.item-name Capital:
                span.item-value= country.capital[0] 


